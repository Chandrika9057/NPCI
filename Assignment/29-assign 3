a = [
  ['chandu', 'cc@gg.com', '123456'],
 ['Teja', 'te@user.com', '789012'],
['Hello', 'email@34','9456'],
['chandu', 'cc@gg.com', '123456'],
 ]

lines_seen = set() 
fi= open('sample.txt', "w")
for line in lines_seen:
    print(line)
    if line not in lines_seen:  
        fi.write(line)
        lines_seen.add(line)
fi.close()
with open('sample.txt','r') as file:
    for line in file:
        l.append(line)
        print(tuple(l))
